<h1>Create an Account</h1>

<%= form_with url: create_account_path, local: true, method: :post do %>
    <div>
        <%= label_tag "username" %><br>
        <%= text_field_tag "username", nil %><br>
        <%= label_tag "password" %><br>
        <%= text_field_tag "password", nil %>
    </div>
    <%= submit_tag "Create Account" %>
<% end %>